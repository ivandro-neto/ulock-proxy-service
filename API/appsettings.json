{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt":{
    "Secret" : "ulock-proxy-service-ulock-proxy-service",
    "Issuer" : "ulock-proxy-service",
    "Audience" : "ulock-proxy-service-audience"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes":{
      "digitalRoute":{
        "ClusterId": "digitalCluster",
        "Match": {
          "Host": "digitaloffice.ucall.co.ao",
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "digitalCluster": {
        "Destinations": {
          "digitalDestination": {
            "Address": "https://10.11.1.236:8030/"
          }
        }
      }
    }
  }
}
